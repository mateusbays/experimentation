      {
        "id": 1,
        "title": "Vibração Overall - Motor Principal",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "influxdb"
            },
            "query": "from(bucket: \"mybucket\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"vibration\")\n  |> filter(fn: (r) => r.equipment_name == \"motor1\")\n  |> filter(fn: (r) => r.direction == \"overall\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  |> yield(name: \"mean\")",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
